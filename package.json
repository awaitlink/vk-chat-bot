{
  "name": "vk-chat-bot",
  "version": "10.5.0",
  "description": "Framework for easy creation of chat bots for VK communities (uses Callback API).",
  "keywords": [
    "api",
    "bot",
    "callback",
    "chat",
    "community",
    "conversation",
    "es6",
    "group",
    "message",
    "sdk",
    "vk"
  ],
  "bugs": "https://github.com/u32i64/vk-chat-bot/issues",
  "repository": {
    "type": "git",
    "url": "https://github.com/u32i64/vk-chat-bot.git"
  },
  "license": "MIT",
  "author": {
    "name": "Artem Varaksa (u32i64)",
    "email": "aymfst@gmail.com"
  },
  "files": [
    "lib/lib.min.js",
    "lib/lib.min.js.map",
    "LICENSE",
    "CHANGELOG.md",
    "README.md"
  ],
  "main": "lib/lib.min.js",
  "scripts": {
    "build": "npm run clean && mkdir lib && rollup -c",
    "clean": "rm -rf lib *.tgz",
    "minify": "uglifyjs lib/lib.js -c -o lib/lib.min.js --source-map 'content=inline'",
    "prepare": "npm run test && npm run build && npm run minify",
    "test": "npm run test:lint && npm run test:ava",
    "test:ava": "ava",
    "test:lint": "standard --verbose | snazzy",
    "fix": "standard --fix"
  },
  "dependencies": {
    "@babel/polyfill": "^7.0.0",
    "@babel/runtime": "^7.0.0",
    "body-parser": "^1.18.3",
    "colors": "^1.3.2",
    "express": "^4.16.3",
    "moment": "^2.22.2",
    "moment-duration-format": "^2.2.2",
    "request": "^2.88.0",
    "request-promise": "^4.2.2"
  },
  "devDependencies": {
    "@babel/core": "^7.0.1",
    "@babel/plugin-transform-runtime": "^7.0.0",
    "@babel/preset-env": "^7.0.0",
    "@babel/register": "^7.0.0",
    "ava": "^1.0.0-beta.8",
    "rollup": "^0.65.2",
    "rollup-plugin-babel": "^4.0.3",
    "rollup-plugin-node-resolve": "^3.4.0",
    "snazzy": "^8.0.0",
    "standard": "^12.0.1",
    "uglify-js": "^3.4.9"
  },
  "engines": {
    "node": ">=8.0.0"
  },
  "ava": {
    "require": [
      "@babel/register"
    ]
  }
}
