{
  "defaultSeverity": "error",
  "extends": ["tslint:recommended"],
  "jsRules": {},
  "rules": {
    "quotemark": [true, "single", "avoid-escape"],
    "arrow-parens": [true, "ban-single-arg-parens"],
    "variable-name": {
      "options": ["check-format", "require-const-for-all-caps"]
    },
    "no-console": [false],
    "object-literal-sort-keys": [false]
  },
  "rulesDirectory": []
}
