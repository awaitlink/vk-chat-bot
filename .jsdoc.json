{
  "plugins": ["plugins/markdown"],
  "recurseDepth": 10,
  "source": {
    "include": [
      "src",
      "src/extra",
      "src/api",
      "package.json",
      "README.md"
    ],
    "includePattern": ".js$"
  },
  "sourceType": "module",
  "tags": {
    "allowUnknownTags": true,
    "dictionaries": ["jsdoc", "closure"]
  },
  "templates": {
    "cleverLinks": false,
    "monospaceLinks": true,
    "useLongnameInNav": false,
    "showInheritedInNav": true,
    "default": {
      "includeDate": false
    }
  },
  "opts": {
    "destination": "./docs/",
    "template": "node_modules/minami",
    "tutorials": "./tutorials/"
  }
}