{
  "plugins": ["plugins/markdown"],
  "recurseDepth": 10,
  "tags": {
    "allowUnknownTags": true,
    "dictionaries": ["jsdoc", "closure"]
  },
  "templates": {
    "cleverLinks": false,
    "monospaceLinks": true,
    "useLongnameInNav": false,
    "showInheritedInNav": true,
    "default": {
      "includeDate": false
    }
  },
  "opts": {
    "destination": "./docs/",
    "template": "node_modules/minami",
    "tutorials": "./tutorials/",
    "private": true
  }
}